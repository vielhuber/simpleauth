{
  "singleQuote": true,
  "overrides": [
    {
      "files": ["**/*.css", "**/*.scss"],
      "options": {
        "singleQuote": false
      }
    }
  ],
  "tabWidth": 4,
  "printWidth": 120,
  "trailingComma": "none",
  "trailingCommaPHP": false,
  "arrowParens": "avoid",
  "phpVersion": "8.3",
  "plugins": ["@prettier/plugin-php"]
}
